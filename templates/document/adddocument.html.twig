{% extends 'base.html.twig' %}

{% block title %}Creation de document{% endblock %}

{% block body %}
<h2> Cr√©e un nouveau document </h2>
<br>
<div style="text-align: center">
    {{ form_start(form) }}
    <div class="form-row">
            <div class="form-label">{{ form_label(form.date) }}</div>
            <div class="form-field">{{ form_widget(form.date) }}</div>
        </div>
        <div class="form-row">
            <div class="form-label">{{ form_label(form.type) }}</div>
            <div class="form-field">{{ form_widget(form.type) }}</div>
        </div>
    <button type="submit" class="submit-button">Enregistrer</button>
    {{ form_end(form) }}
</div>
{% endblock %}
